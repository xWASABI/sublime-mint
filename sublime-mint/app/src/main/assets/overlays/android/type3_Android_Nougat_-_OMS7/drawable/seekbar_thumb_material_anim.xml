<?xml version="1.0" encoding="utf-8"?>
<animated-selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape shape="rectangle">
            <corners android:radius="20.0dip" />
            <gradient
				android:startColor="#90505050" 
				android:endColor="#303030"
				android:angle="90.0" 
				android:centerY="0.9" 
				android:centerColor="#303030"
				type="linear" />
            <size android:height="12.0dip" android:width="20.0dip" />
          <stroke android:width="1.0dip" android:color="@*android:color/Indigo_800" />
        </shape>
    </item>
    <item android:state_pressed="true" android:id="@*android:id/pressed">
        <shape shape="rectangle">
            <corners android:radius="20.0dip" />
            <gradient android:startColor="@*android:color/Purple_800" android:endColor="@*android:color/Teal_700" android:angle="90.0" android:centerY="0.9" android:centerColor="@*android:color/Teal_700" type="linear" />
