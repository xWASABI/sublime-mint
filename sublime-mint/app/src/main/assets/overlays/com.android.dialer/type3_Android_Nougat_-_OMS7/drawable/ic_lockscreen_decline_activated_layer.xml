<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape
			android:shape="oval">
			<solid 
				android:color="@*android:color/Pink_700" />

			<stroke
				android:width="1dp"
				android:color="@*android:color/Teal_700" />
		</shape>
	</item>	
	
	<item>
        <bitmap
            android:gravity="center"
            android:src="@*com.android.dialer:drawable/fab_ic_end_call"
            android:tint="@*com.android.dialer:color/glowpad_widget_active_color" />
    </item>
</layer-list>
