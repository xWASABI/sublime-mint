<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  android:orientation="horizontal"
  android:background="@*android:drawable/toast_frame" 
  android:clickable="true" 
  android:layout_width="fill_parent" 
  android:layout_height="wrap_content" 
  android:baselineAligned="false"
  xmlns:android="http://schemas.android.com/apk/res/android">
<FrameLayout 
    android:id="@*com.google.android.apps.plus:id/navigation_home" 
	android:background="#00000000"
    android:layout_width="0.0dip"
	android:layout_height="@*com.google.android.apps.plus:dimen/navigation_bar_height_horizontal" 
    android:layout_weight="1.0">
<TextView 
    android:layout_gravity="center" 
	android:textColor="#FFFFFF"
	android:id="@*com.google.android.apps.plus:id/navigation_home_text"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
	android:text="@*com.google.android.apps.plus:string/home_navigation_item"
	android:drawableLeft="@*com.google.android.apps.plus:drawable/quantum_ic_home_white_24"
	android:contentDescription="@*com.google.android.apps.plus:string/home_navigation_item"
	style="@*com.google.android.apps.plus:style/BottomBarItemHorizontal" />
</FrameLayout>
<FrameLayout 
	android:id="@*com.google.android.apps.plus:id/navigation_collections" 
	android:background="#00000000" 
    android:layout_width="0.0dip"
	android:layout_height="@*com.google.android.apps.plus:dimen/navigation_bar_height_horizontal" 
    android:layout_weight="1.0">
<TextView 
    android:layout_gravity="center" 
	android:textColor="#FFFFFF"
	android:id="@*com.google.android.apps.plus:id/navigation_collections_text"
    android:layout_width="wrap_content" 
    android:layout_height="wrap_content" 
	android:text="@*com.google.android.apps.plus:string/collections_navigation_item" 
  	android:drawableLeft="@*com.google.android.apps.plus:drawable/quantum_ic_google_collections_white_24" 
  	android:contentDescription="@*com.google.android.apps.plus:string/collections_navigation_item"
  	style="@*com.google.android.apps.plus:style/BottomBarItemHorizontal" />
</FrameLayout>
<FrameLayout 
	android:id="@*com.google.android.apps.plus:id/navigation_communities"
	android:background="#00000000" 
    android:layout_width="0.0dip" 
	android:layout_height="@*com.google.android.apps.plus:dimen/navigation_bar_height_horizontal" 
    android:layout_weight="1.0">
<TextView 
    android:layout_gravity="center" 
	android:textColor="#FFFFFF"
	android:id="@*com.google.android.apps.plus:id/navigation_communities_text" 
    android:layout_width="wrap_content" 
    android:layout_height="wrap_content"
	android:text="@*com.google.android.apps.plus:string/communities_navigation_item"
	android:drawableLeft="@*com.google.android.apps.plus:drawable/quantum_ic_communities_white_24" 
	android:contentDescription="@*com.google.android.apps.plus:string/communities_navigation_item" 
	style="@*com.google.android.apps.plus:style/BottomBarItemHorizontal" />
</FrameLayout>
<LinearLayout
    android:gravity="center"
    android:orientation="horizontal"
	android:id="@*com.google.android.apps.plus:id/navigation_notifications" 
	android:background="#00000000" 
	android:paddingTop="@*com.google.android.apps.plus:dimen/navigation_bar_y_padding"
	android:paddingBottom="@*com.google.android.apps.plus:dimen/navigation_bar_y_padding"
    android:layout_width="0.0dip" 
	android:layout_height="@*com.google.android.apps.plus:dimen/navigation_bar_height_horizontal" 
    android:layout_weight="1.0">
<FrameLayout
  android:layout_gravity="center" 
  android:background="@android:color/transparent" 
  android:layout_width="24.0dip"
  android:layout_height="24.0dip">
<ImageView
    android:layout_gravity="center"
	android:id="@*com.google.android.apps.plus:id/navigation_notifications_icon" 
    android:layout_width="wrap_content" 
    android:layout_height="wrap_content" 
	android:src="@*com.google.android.apps.plus:drawable/quantum_ic_notifications_white_24" 
    android:importantForAccessibility="no" />
<ImageView 
	android:id="@*com.google.android.apps.plus:id/navigation_notifications_count" 
    android:visibility="gone" 
    android:importantForAccessibility="no"
	style="@*com.google.android.apps.plus:style/BottomBarNotificationCounter" />
</FrameLayout>
<TextView 
	android:textSize="@*com.google.android.apps.plus:dimen/text_size_12" 
    android:textColor="#FFFFFF" 
    android:ellipsize="end" 
    android:gravity="center"
    android:layout_gravity="center"
	android:id="@*com.google.android.apps.plus:id/navigation_notifications_text" 
    android:paddingLeft="5.0dip" 
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
	android:text="@*com.google.android.apps.plus:string/notifications_navigation_item" 
    android:singleLine="true" />
    </LinearLayout>
</LinearLayout>
