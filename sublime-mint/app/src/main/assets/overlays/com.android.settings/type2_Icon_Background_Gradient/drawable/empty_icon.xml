<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
	<item>
		<shape
			android:shape="rectangle">
			<corners
				android:radius="20.0dip"/>
			<gradient
				android:startColor="@android:color/transparent"
				android:endColor="@*android:color/Teal_700"
				android:centerX="50%"
				android:centerY="50%"
				android:gradientRadius="100%"
				android:type="radial"/>
			<stroke
				android:color="@*android:color/Teal_700"
				android:width="0.0dip"/>
		</shape>
	</item>
</layer-list>
