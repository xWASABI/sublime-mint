<?xml version="1.0" encoding="utf-8"?>
<com.google.android.apps.contacts.editor.RawContactEditorView 
  android:orientation="vertical"
  android:id="@*com.google.android.contacts:id/raw_contacts_editor_view" 
  android:background="@*android:color/Indigo_700"
  android:visibility="invisible"
  android:layout_width="fill_parent" 
  android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
<RelativeLayout 
		android:gravity="center" 
		android:id="@*com.google.android.contacts:id/header_container" 
	    android:background="@*com.google.android.contacts:color/quickcontact_header_background"
		android:layout_width="fill_parent"
		android:layout_height="162dp">
	<include layout="@*com.google.android.contacts:layout/photo_editor_view" />
    </RelativeLayout>
    <View 
		android:focusable="true" 
		android:focusableInTouchMode="true" 
		android:layout_width="0.0dip" 
		android:layout_height="0.0dip" />
    <ScrollView 
		android:fadingEdge="none" 
		android:layout_width="fill_parent" 
		android:layout_height="fill_parent" 
		android:fillViewport="true">
        <LinearLayout 
		android:orientation="vertical" 
		android:layout_width="fill_parent" 
		android:layout_height="fill_parent">
            <include layout="@*com.google.android.contacts:layout/editor_account_header" />
            <include layout="@*com.google.android.contacts:layout/contact_editor_fields" />
        </LinearLayout>
    </ScrollView>
</com.google.android.apps.contacts.editor.RawContactEditorView>
