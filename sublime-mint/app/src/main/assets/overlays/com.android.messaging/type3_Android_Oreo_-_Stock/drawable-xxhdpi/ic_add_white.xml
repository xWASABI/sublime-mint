<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape
			android:shape="oval">
			<solid 
				android:color="@*android:color/Pink_700" />

			<stroke
				android:width="4dp"
				android:color="@*android:color/Teal_700" />
			
			<size 
				android:width="52dp"
				android:height="52dp"/>
			
		</shape>
	</item>	

	<item>
        <bitmap
            android:gravity="center"
            android:src="@*android:drawable/ic_suggestions_add"
            android:tint="@*android:color/white" />
    </item>
</layer-list>
