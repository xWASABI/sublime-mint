<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="Base.Theme" parent="@android:style/Theme.Material">	
	</style>
	<style name="Base.Theme.Instagram" parent="@*com.instagram.android:style/Base.Theme">	
		<item name="android:backgroundTint">@*android:color/Indigo_700</item>		
		<item name="android:windowLightStatusBar">true</item>	
		<item name="android:windowFullscreen">true</item>
		<item name="android:textColorLink">@*android:color/Teal_700</item>
		<item name="android:colorBackground">@*android:color/Indigo_700</item>
		<item name="android:colorBackgroundFloating">@*android:color/Indigo_700</item>
        <item name="android:windowBackground">@*android:color/Indigo_700</item>
		<item name="android:colorPrimaryDark">#f44</item>
		<item name="android:statusBarColor">@*android:color/Red_800</item>
        <item name="android:windowNoTitle">true</item>
		<item name="android:soundEffectsEnabled">false</item>
        <item name="android:windowContentOverlay">@null</item>
		<item name="android:buttonStyle">@*com.instagram.android:style/ButtonStyle</item>
        <item name="android:autoCompleteTextViewStyle">@*com.instagram.android:style/IgAutoCompleteTextView</item>
        <item name="android:checkboxStyle">@*com.instagram.android:style/IgTextboxStyle</item>
        <item name="android:editTextStyle">@*com.instagram.android:style/EditTextStyle</item>
        <item name="android:tabWidgetStyle">@*android:style/Widget.TabWidget</item>
        <item name="android:textViewStyle">@*com.instagram.android:style/TextViewStyle</item>
        <item name="android:dialogTheme">@*com.instagram.android:style/IgDialog</item>
        <item name="*com.instagram.android:glyphColorPrimary">@*android:color/white</item>
        <item name="*com.instagram.android:glyphColorSecondary">@*android:color/Blue_700</item>
        <item name="*com.instagram.android:glyphColorSecondaryActive">@*android:color/Teal_700</item>
        <item name="*com.instagram.android:textColorPrimary">@*android:color/Blue_700</item>
        <item name="*com.instagram.android:textColorSecondary">@*android:color/Blue_700</item>
        <item name="*com.instagram.android:textColorTertiary">@*android:color/Blue_700</item>
        <item name="*com.instagram.android:textColorSelected">@*android:color/white</item>
        <item name="*com.instagram.android:textColorBoldLink">@*android:color/white</item>
        <item name="*com.instagram.android:textColorRegularLink">@*android:color/Teal_700</item>
        <item name="*com.instagram.android:textColorHyperlink">@*android:color/Teal_700</item>
        <item name="*com.instagram.android:dividerColor">@*android:color/transparent</item>
        <item name="*com.instagram.android:backgroundColorPrimary">@*android:color/Pink_700</item>
        <item name="*com.instagram.android:backgroundColorPrimaryDark">@*android:color/Red_800</item>
        <item name="*com.instagram.android:backgroundColorSecondary">@*android:color/Indigo_700</item>
        <item name="*com.instagram.android:backgroundColorHighlight">@*android:color/Blue_700</item>
        <item name="*com.instagram.android:backgroundColorHighlightPressed">@*android:color/Teal_700</item>
		<!--
        <item name="*com.instagram.android:backgroundDrawable">@*com.instagram.android:drawable/bg_simple_row</item>
        -->
		<item name="*com.instagram.android:backgroundDrawable">@*android:drawable/toast_frame</item>
		<item name="*com.instagram.android:backgroundNotificationBadge">@*com.instagram.android:drawable/notification_bubble_whiteout</item>
        <item name="*com.instagram.android:defaultSpinnerStyle">@*com.instagram.android:style/SpinnerTextStyle.Whiteout</item>
        <item name="*com.instagram.android:segmentedUnderlineWidth">@*com.instagram.android:dimen/segmented_underline_width_whiteout</item>
        <item name="*com.instagram.android:igSwitchStyle">@*com.instagram.android:style/IgSwitchStyle</item>
    </style>
	
	<style name="TabBarButton">
        <item name="*com.instagram.android:normal">@*android:color/white</item>
        <item name="*com.instagram.android:active">@*android:color/Teal_700</item>
    </style>
    <style name="TabBarButton.White" parent="@*com.instagram.android:style/TabBarButton">
        <item name="*com.instagram.android:normal">@*android:color/white</item>
        <item name="*com.instagram.android:active">@*android:color/Teal_700</item>
    </style>
	<!--
	<style name="IgPrimaryText" parent="@android:style/Widget.TextView">
		<item name="capitalize">false</item>
		<item name="medium">true</item>
	</style>
	-->
	<style name="Body" parent="@*com.instagram.android:style/IgPrimaryText">
		<item name="android:textSize">@*com.instagram.android:dimen/font_medium</item>
		<item name="android:textColor">#f44</item>
		<item name="android:lineSpacingMultiplier">1.2</item>
	</style>
	<style name="BodyDetail" parent="@*com.instagram.android:style/Body">
		<item name="android:textColor">#ef1</item>
		<item name="android:lineSpacingMultiplier">5.3</item>
	</style>
	
</resources>
